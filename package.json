{
  "name": "vue3-boilerplate",
  "version": "0.0.1",
  "description": "a vue3 beta boilerplate",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist/**",
    "test:unit": "cross-env NODE_ENV=test karma start test/unit/karma.conf.js --single-run",
    "dev": "webpack-dev-server --inline --progress --config ./config/webpack.dev.js",
    "build:stage": "cross-env NODE_ENV=prod:stage yarn clean && webpack --config config/webpack.prod.js"
  },
  "sideEffects": [
    "*.css",
    "src/**/*.vue"
  ],
  "repository": "git@github.com:Styx11/vue3-boilerplate.git",
  "author": "styx11 <1654630248@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/test-utils": "^2.0.0-beta.8",
    "babel-loader": "^8.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^1.2.0",
    "mocha": "^8.2.0",
    "rimraf": "^3.0.2",
    "svg-sprite-loader": "^5.0.0",
    "ts-import-plugin": "^1.6.6",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "url-loader": "^4.1.1",
    "vue-loader": "^16.0.0-beta.8",
    "vue-style-loader": "^4.1.2",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "4.1.0"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "ant-design-vue": "^2.0.0-beta.10",
    "vue": "^3.0.2",
    "vue-router": "^4.0.0-rc.1"
  }
}
